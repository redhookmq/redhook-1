{
  "name": "hugo-starter",
  "version": "1.0.0",
  "description": "This is a boilerplate for using [Hugo](https://gohugo.io/) as a static site generator and Gulp + Webpack as your asset pipeline",
  "main": "index.js",
  "scripts": {
    "hugo": "gulp hugo",
    "build": "rm -rf ./dist && cross-env NODE_ENV=production gulp build",
    "build-preview": "rm -rf ./dist && cross-env NODE_ENV=production gulp build-preview",
    "dev": "rm -rf ./dist && gulp dev",
    "lint": "eslint src"
  },
  "author": "",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "isotope-layout": "^3.0.4",
    "isotope-packery": "^2.0.0",
    "jquery": "^3.5.0",
    "jquery.easing": "^1.4.1",
    "lodash": "^4.17.4",
    "modernizr": "^3.5.0",
    "postal": "^2.0.5"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.6",
    "browser-sync": "^2.13.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.11.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cache-bust": "^1.1.0",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.1.1",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-util": "^3.0.7",
    "imports-loader": "^0.7.1",
    "modernizr-loader": "^1.0.1",
    "run-sequence": "^2.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.6.0",
    "yarn": "^0.27.5"
  }
}
